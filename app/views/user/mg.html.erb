  <h1>ユーザー一覧</h1>
  <p>ここはユーザー一覧です(管理画面想定)</p>

  <div class="user-index">
    <table>
      <tr><th>id</th><th>user-name</th><th>email</th><th>password</th><th>img_name</th></tr>
      <% @users.each do |user| %>
      <tr>
        <td>
          <%= user.id %>
        </td>
        <td>
          <%= link_to(user.name, "/user/#{user.id}") %>
        </td>
        <td>
          <%= user.email %>
        </td>
        <td>
          <%= user.password %>
        </td>
        <td>
          <%= user.img_name %>
        </td>
      </tr>
      <% end %>
    </table>
  </div>

  <div class="user-index">
    <table>
      <tr><th>id</th><th>content</th><th>user_id</th><th>created_at</th><th>updated_at</th></tr>
      <% @posts.each do |post| %>
      <tr>
        <td>
          <%= post.id %>
        </td>
        <td>
          <%= link_to(post.content, post_path(post.id)) %>
        </td>
        <td>
          <%= post.user_id %>
        </td>
        <td>
          <%= post.created_at %>
        </td>
        <td>
          <%= post.updated_at %>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
